@model object[]



<form class="form-horizontal form-label-left">
    <div class="row">
        <div id="11" style="text-align:right;">
        </div>
    </div>
    <div class="row">
        <div class="col-md-6 col-sm-6 col-xs-6">
            <div class="form-group">
                <br />
                <label>Dependencia Jerárquica (*)</label>
                @Html.DropDownList("cboJerarquia", (SelectList)ViewBag.CampJerar, "Seleccione", new { @class = "form-control" })
            </div>
        </div>
        <div class="col-md-6 col-sm-6 col-xs-6">
            <br />
            <div class="form-group">
                <div>
                    <label class="col-md-7 col-sm-7 col-xs-6"> Und. Organizacional </label>
                    <div class="col-md-5 col-sm-5 col-xs-6 text-right">
                    </div>
                </div>
                <select id="cbounidsupe" class="form-control">
                    <option>Seleccione</option>
                </select>
            </div>
        </div>
    </div>

    @*<div class="ln_solid"></div>*@

    <div class="row">
        <div class="border-section">
            <div class="col-md-6 col-sm-6 col-xs-6">
                <div class="form-group Valcod">
                    <label>Código (*)</label>
                    <input id="txt-cod-Grupo" type="text" name="codigo" class="form-control" @*pattern="^([A-Za-z0-9])+"*@ maxlength="8" placeholder="Código" onkeypress="validarCodigoAll(event)">
                    <div class="notifry_error" style="background-color:#4CA4DE;color:white;text-align:center;">
                    </div>
                </div>
            </div>
            <div class="col-md-6 col-sm-6 col-xs-6">
                <div class="form-group Valdes">
                    <label>Descripción (*)</label>
                    <input id="txt-desc-Grupo" type="text" class="form-control" placeholder="Descripción " onkeypress="validarDescripcionAll(event)">
                    <div class="notifry_error" style="background-color:#4CA4DE;color:white;text-align:center;"></div>
                </div>
            </div>
        </div>
    </div>
    <br />
    @*<div class="ln_solid"></div>*@
    <div id="containerCampose">
    </div><div class="col-md-6 col-sm-6 col-xs-6">
        <div class="form-group" id="IdGroup" hidden>
            <label>IdGroup</label>
            <input id="txtIdGroup" type="hidden" class="form-control">
        </div>
    </div>
    <script>


        BuscarUnidades();


    </script>
</form>
