
<form class="form-horizontal-dropzone form-label-left">

    <div class="row">
        <div class="col-md-6 col-sm-6 col-xs-6">
        </div>
        <div style="text-align:right;">
            <label id="_lbl_"></label>
            <input type="checkbox" id="chk-activo-JorDia" class="js-switch" checked />
        </div>
    </div>

    <div class="row">
        <div class="col-md-6 col-sm-6 col-xs-6">
            <div class="form-group">
                <label>Tipo de jornada (*)</label>
                <select class="form-control" id="TipoJor">
                    <option selected>Seleccione</option>
                </select>
            </div>
        </div>
        <div class="col-md-6 col-sm-6 col-xs-6">
            <div class="form-group">
                <label>Tipo de Refrigerio (*)</label>
                <select class="form-control" id="cboRef"></select>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="form-group">
            <div class="col-md-6 col-sm-6 col-xs-6">
                <div class="form-group">
                    <label>Dependencia Jerárquica (*)</label>
                    <select class="form-control" id="cboJerar"></select>
                </div>
            </div>


            <div class="col-md-6 col-sm-6 col-xs-6">
                <div class="form-group">
                    <label>Unid. Organizacional (*)</label>
                    <select class="form-control" id="cboUndOrg" disabled></select>
                </div>
            </div>

        </div>
    </div>

    <div class="row">
        <div class="border-section">
            <div class="col-md-6 col-sm-6 col-xs-6">
                <div class="form-group" id="Valcod">
                    <label>Código (*)</label>
                    <input type="text" id="txt_cod_Jor" name="codigo" class="form-control" placeholder="Código" @*pattern="^([A-Za-z0-9])+"*@ maxlength="8" onkeypress="validarCodigoAll(event)">
                    <div class="notifry_error" style="background-color:#4CA4DE;color:white;text-align:center;"></div>
                </div>
            </div>
            <div class="col-md-6 col-sm-6 col-xs-6">
                <div class="form-group" id="Valdes">
                    <label>Descripción (*)</label>
                    <input type="text" id="txt_desc_Jor" class="form-control" placeholder="Descripción" onkeypress="validarDescripcionAll(event)">
                    <div class="notifry_error" style="background-color:#4CA4DE;color:white;text-align:center;"></div>
                </div>
            </div>
        </div>
    </div>
    <br />
    <div class="row">
        <div class="form-group">
            <div class="col-md-6 col-sm-6 col-xs-6">

                <input type="checkbox" id="chck_cambdia" class="flat" /> Jornada con Cambio de Día
                <br /> <br />
                <div id="Pertenece_Jor" hidden>
                    <input type="checkbox" id="chck_perteneceJor" class="flat" /> Jornada Pertenece a la Fecha Fin de la Jornada (al Día Siguiente)
                </div> <br /> <br />
            </div>
            <div class="col-md-6 col-sm-6 col-xs-6">
                <label>Color de la Jornada</label>
                <div class="input-group demo2" id="Valcol">
                    <input type="text" id="txtcolor" value="#e01ab5" class="form-control" />
                    <span class="input-group-addon"><i></i></span>
                </div>
            </div>
        </div>
    </div>


    <div class="x_panel">
        <div class="x_title">
            <h2>Detalle de Intervalos</h2>
            <div style="text-align:right;">
                <ul class="nav navbar-right panel_toolbox">
                    <li>
                        <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                    </li>
                </ul>
            </div>
            <div class="clearfix"></div>
        </div>
        <div id="oculto">
            <div class="x_content" id="aIntervalos" aria-expanded="false">
                <div class="row">
                    <div class="col-md-6 col-sm-6 col-xs-6" id="group">
                        <div class="form-group">
                            <label>Turno (*)</label>
                            <select class="form-control" id="TipoTurn">
                                <option value="2">Seleccione</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-md-6 col-sm-6 col-xs-6" id="group">
                        <div class="form-group">
                            <label>Tipo de Intervalo (*)</label>
                            <select class="form-control" id="TipoInter">
                                <option value="2">Seleccione</option>
                            </select>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-6 col-sm-6 col-xs-6">
                        <div class="col-md-8 pl-0">
                            <div class="form-group">
                                <label>Hora de Inicio (*)</label>
                                <input type="time" id="tiempo_inic" min="00:00"
                                       max="24:00" accept="hh:mm" value="00:00" class="form-control" />
                            </div>
                        </div>
                        <div class="col-md-4 p-0">
                            <label>&nbsp;</label>
                            <div>
                                <input type="checkbox" class="icheckbox_flat-blue" id="bitDiaSqtIni">
                                <label>Día Sgt.</label>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6 col-sm-6 col-xs-6">
                        <div class="col-md-8 pl-0">
                            <div class="form-group">
                                <label>Hora de Fin (*)</label>
                                <input type="time" id="tiempo_fin" min="00:00"
                                       max="24:00" accept="hh:mm" value="00:00" class="form-control" />
                            </div>
                        </div>
                        <div class="col-md-4 p-0">
                            <label>&nbsp;</label>
                            <div>
                                <input type="checkbox" class="icheckbox_flat-blue" id="bitDiaSqtFin">
                                <label>Día Sgt.</label>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-6 col-sm-6 col-xs-6" id="group">
                        <div class="form-group">
                            <label>Duración </label>
                            <input type="time" id="tiempo_dura" min="00:00"
                                   max="24:00" accept="hh:mm" value="00:00" class="form-control" />
                        </div>
                    </div>
                    <!--modificado HGM 25.10.2021-->
                    <div class="col-md-6 col-sm-6 col-xs-6" id="group">
                        <div class="form-group"  id="tiempo_tole_f">
                            <label>Tolerancia </label>
                            <input type="time" id="tiempo_tole" min="00:00"
                                   max="24:00" accept="hh:mm" value="00:00" class="form-control" />
                        </div>
                    </div>

                </div>

                <div class="row">
                    <div class="col-md-6 col-sm-6 col-xs-6" id="group">
                        <div id="checks_pri_ult"  class="form-inline" hidden>
                            <input type="checkbox" id="chck_pri" class="flat" /> Primero
                            &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;  
                            <input type="checkbox" id="chck_ult" class="flat" /> Último
                            <br /> <br />
                        </div>
                        <div id="cons_trab" hidden>
                            <br />
                            <input type="checkbox" id="consTrab" class="flat" /> Considerar Ref. como Trabajado (HT)
                            <br /> <br />
                        </div>
                    </div>
                    <div class="col-md-6 col-sm-6 col-xs-6" id="group">
                        <div id="consi_max" hidden>
                            <div class="form-group">
                                <label> Duración Máxima </label> @*Tiempo Considerado (max)*@
                                <input type="time" id="tiempo_cons_max" min="00:00"
                                       max="24:00" accept="hh:mm" value="00:00" class="form-control" />
                            </div>
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-6 col-sm-6 col-xs-6" id="group">
                        <!-- COMBO Tipo de Servicio (*)-->
                        <div id="TipServCom" hidden>
                            <label>Tipo de Servicio</label>
                            <select class="form-control" id="TipoServC">
                                <option value="2">Seleccione</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-md-3 col-sm-6 col-xs-6">
                    </div>
                    <div class="col-md-3 col-sm-6 col-xs-6">
                        <i style="float:right;" id="btn-limpiar-Intrevalos" class="fa fa-trash-o fa-2x" title="Limpiar"></i>
                    </div>
                </div>

                <div class="ln_solid"></div>
                <div class="row">
                    <div class="col-md-8 col-sm-8 col-xs-8">
                        <h3>Intervalos Agregados a la Jornada</h3>
                    </div>
                    <div class="col-md-4 col-sm-4 col-xs-4">
                        <center>
                            <label class="btn btn-primary" id="btn-save-changes-Intrevalos" title="Agregar Intervalo"><i class="fa fa-plus fa-2x"></i></label>
                            <label class="btn btn-danger" id="btn-clear-Intrevalos" title="Quitar Intervalos"><i class="fa fa-trash-o fa-2x"></i></label>
                        </center>
                    </div>
                </div>
                <br />
                <label style="color:red;">Permite Agregar un Máximo de 15 Intervalos</label>
                <br />
                <div class="row">
                    <div class="col-md-12 col-sm-12 col-xs-12">
                        <table id="TablaIntervalso" class="table table-striped table-bordered table-responsive pre-wrap" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th>Intervalo</th>
                                    <th>Tipo de Intervalo</th>
                                    <th style="display:none">-</th>
                                    <th>Duración<br />(HH:MM)</th>
                                    <th style="display:none">-</th>
                                    <th style="display:none">-</th>
                                    <th style="display:none">-</th>
                                    <th>Tolerancia</th>
                                    <th>Turno</th>
                                    <th>Orden</th>
                                    <th style="display:none">-</th>
                                    <th style="display:none">-</th>
                                    <th>Acción</th>
                                    <th style="display:none">-</th>
                                    <th style="display:none">-</th>

                                    <th style="display:none">ID TIP SERV</th> <!--CAMPO AÑADIDO-->
                                    <th>Tipo de Servicio</th> <!-- CAMPO AÑADIDO-->
                                </tr>
                            </thead>
                            <tbody id="Tabla_Intervalos"></tbody>

                        </table>
                    </div>

                </div>
            </div>
        </div>
    </div>

    @*<div class="ln_solid">
        </div>*@
    <div id="containerCampose"></div>
    <br />
    <br /> <br />
    <br />
    <input type="hidden" id="CantArrInter" />
    <input type="hidden" id="primer" />
    <input type="hidden" id="end" />
    <input type="hidden" id="end2" />

</form>

